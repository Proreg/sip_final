<tr>
	<td>
		<center>
			<select name="new_items[][ins_inspector_product]" id="item_ins_product" onchange="populateKm(this, this.id)" class="ui fluid search dropdown">>
				<%= options_for_select(InsInspectorProduct.where.not(ins_spending_type_id: 1).map{|s|[s.description, s.id]}.insert(0, "") , { :class => "ui fluid search dropdown"} )%>
			</select>
		</center>
	</td>
	<td>
		<center>
			<input type="text" name="new_items[][amount]" id="item_amount" onblur="calcTotal(this.id);"/>
		</center>
	</td>
	<td>
		<center>
			<input type="text" name="new_items[][unitary_value]" id="item_unitary_value" onblur="calcTotal(this.id);" class="money_value" />
		</center>
	</td>
	<td>
		<center>
			<input type="text" name="new_items[][total_value]" id="item_total_value" class="money_value" onblur="calcTotal(this.id);"  />
		</center>
	</td>
	<td>
		<center>
			<input type="text" name="new_items[][deductible]" id="item_deductible" />
		</center>
	</td>
	<td>
		<center>
			<input type="text" name="new_items[][ins_notes]" />
		</center>
	</td>
	<td>
		<center>
			<a href="#" class="remove_field tiny button alert">Remover</a>
		</center>
	</td>	
</tr>

<script>
controlIdNumbers();

jQuery(function($){
	$(".money_value").maskMoney({symbol:'R$ ',
	showSymbol:true, thousands:'.', decimal:',', symbolStay: true});
	
});	
</script>
